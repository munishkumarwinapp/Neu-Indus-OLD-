                            android:text="Bill Disc" />

                        <EditText
                            android:id="@+id/sm_billDisc"
                            style="@style/barcode_edittext"
                            android:layout_width="match_parent"
                            android:layout_height="35dp"
                            android:layout_marginLeft="17dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="2dp"
                            android:gravity="center|center_vertical"
                            android:hint="0"
                            android:inputType="numberDecimal" >
                        </EditText>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:baselineAligned="false"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginRight="2dp"
                android:layout_marginTop="2dip"
                android:layout_weight="2"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/sm_signature_layout"
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="2dip"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/sm_sign_iv"
                        android:layout_width="wrap_content"
                        android:layout_height="28dp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="4dp"
                        android:contentDescription="@string/title_bar_left_button"
                        android:src="@mipmap/ic_summ_sign" />

                    <ImageView
                        android:id="@+id/sm_signature"
                        android:layout_width="0dip"
                        android:layout_height="40dip"
                        android:layout_marginBottom="1dip"
                        android:layout_marginLeft="6dip"
                        android:layout_marginTop="1dip"
                        android:layout_weight=".7"
                        android:contentDescription="@string/title_bar_left_button"
                        android:background="@drawable/labelbg" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dip"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="2dip"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/sm_loc_iv"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="1dp"
                        android:contentDescription="@string/title_bar_left_button"
                        android:src="@mipmap/ic_summ_location" />

                    <EditText
                        android:id="@+id/sm_location"
                        android:layout_width="0dip"
                        android:layout_height="40dip"
                        android:layout_gravity="center"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="1dp"
                        android:layout_weight=".7"
                        android:background="@drawable/labelbg"
                        android:focusable="false"
                        android:gravity="center"
                        android:hint="Location"
                        android:inputType="none"
                        android:textColor="#3a3a3c"
                        android:textSize="10sp"
                        android:typeface="sans" >
                    </EditText>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_marginRight="2dp"
                android:layout_marginTop="1dip"
                android:layout_weight="2"
                android:gravity="center"
                android:id="@+id/linearlayout2"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginBottom="1dp"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="1dp" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="Sub Total" />

                    <EditText
                        android:id="@+id/sm_subTotal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/labelbg"
                        android:focusable="false"
                        android:gravity="center"
                        android:hint="0.00" />
                    <EditText
                        android:id="@+id/sm_subTotal_inclusive"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/labelbg"
                        android:focusable="false"
                        android:gravity="center"
                        android:hint="0.00"
                        android:visibility="gone"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dip"
                    android:layout_marginBottom="1dp"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="1dp" >

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="Tax" />

                    <EditText
                        android:id="@+id/sm_tax"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/labelbg"
                        android:focusable="false"
                        android:gravity="center"
                        android:hint="0.0000" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="1dp" >

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="Net Total" />

                    <EditText
                        android:id="@+id/sm_netTotal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="2dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/labelbg"
                        android:focusable="false"
                        android:gravity="center"
                        android:hint="0.00" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="2dp"
                android:layout_marginTop="1dip"
                android:layout_weight="2"
                android:gravity="bottom"
                android:visibility="gone"
                android:id="@+id/linearlayout3"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginBottom="1dp"
                    android:layout_marginRight="2dp"
                    android:layout_marginTop="1dp" >

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="Total Qty" />

                    <EditText
                        android:id="@+id/sm_TotalQty"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/labelbg"
                        android:focusable="false"
                        android:gravity="center"
                        android:hint="0.00" />
                </LinearLayout>


            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <!-- <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.2"
        android:gravity="center"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/sm_save"
            style="@style/button_style"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginLeft="6dp"
            android:layout_marginRight="6dp"
            android:text="Save" />
    </LinearLayout> -->

</LinearLayout>