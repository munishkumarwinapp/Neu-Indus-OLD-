<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ss_listitem_layout"
    style="@style/list_item_style"
    android:layout_width="1320dp"
    android:layout_height="wrap_content"
    android:baselineAligned="false"
    android:orientation="vertical" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="80dip" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dip"
            android:layout_marginBottom="3dp"
            android:background="@drawable/listbg_patch"
            android:baselineAligned="false"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:baselineAligned="false"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/ss_slno_layout"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="visible" >

                    <TextView
                        android:id="@+id/ss_slid"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:visibility="invisible" />

                    <TextView
                        android:id="@+id/slNo"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="3dp"
                        android:gravity="center"
                        android:text="SlNo"
                        android:textColor="@color/textview_text_color"
                        android:textSize="16dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/ss_slno"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginLeft="5dip"
                        android:gravity="center"
                        android:text="slno"
                        android:textColor="@color/textview_text_color"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <!--<LinearLayout
                    android:id="@+id/ss_prodcode_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.1"
                    android:gravity="left|center"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/ss_prodcode"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="ProductCode"
                        android:textColor="@color/textview_text_color"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </LinearLayout>-->

                <LinearLayout
                    android:id="@+id/ss_name_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left|center" >

                    <TextView
                        android:id="@+id/ss_name"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:gravity="left|center"
                        android:maxLines="2"
                        android:text="Nameaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
                        android:textColor="@color/textview_text_color"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:baselineAligned="false"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/ss_prodcode_layout"
                    android:layout_width="50dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:gravity="left"
                    android:paddingLeft="15dp"
                    android:visibility="visible">

                    <TextView
                        android:id="@+id/ss_prodcode"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="3dp"
                        android:gravity="center"
                        android:text="abcdefg"
                        android:textColor="@color/textview_text_color"
                        android:textSize="16dp"
                        android:textStyle="italic"/>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_cqty_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:gravity="right">

                    <!--
                     <ImageButton
                    android:id="@+id/cQuantity_minus"
                     android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight=".4"
                    android:background="@drawable/left_separator"
                    android:gravity="center"
                    android:src="@drawable/active_minus" />
                    -->

                    <TextView
                        android:id="@+id/ss_c_qty"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="0"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textSize="14sp"/>

                    <!--
                     <ImageButton
                    android:id="@+id/cQuantity_plus"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight=".4"
                    android:background="@drawable/right_separator"
                    android:gravity="center"
                    android:src="@drawable/active_plus" />
                    -->
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_lqty_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right" >

                    <!--
                     <ImageButton
                    android:id="@+id/lQuantity_minus"
                      android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight=".4"
                    android:background="@drawable/left_separator"
                    android:gravity="center"
                    android:src="@drawable/active_minus" />
                    -->

                    <TextView
                        android:id="@+id/ss_l_qty"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="0"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textSize="14sp" />

                    <!--
                     <ImageButton
                    android:id="@+id/lQuantity_plus"
                      android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight=".4"
                    android:background="@drawable/right_separator"
                    android:gravity="center"
                    android:src="@drawable/active_plus" />
                    -->
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_qty_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right" >

            

                    <TextView
                        android:id="@+id/ss_qty"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="0"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textSize="14sp" />

                    <!--
                     <ImageButton
                    android:id="@+id/quantity_plus"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight=".4"
                    android:background="@drawable/right_separator"
                    android:gravity="center"
                    android:src="@drawable/active_plus" />
                    -->
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_deli_qty_layout"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/ss_deli_qty"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="3dp"
                        android:gravity="center"
                        android:textColor="@color/textview_text_color"
                        android:textSize="16dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_foc_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right" >

                    <TextView
                        android:id="@+id/ss_foc"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="Foc"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_cprice_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/ss_cprice"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text=""
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_price_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right" >

                    <TextView
                        android:id="@+id/ss_price"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="Price"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_total_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right" >

                    <TextView
                        android:id="@+id/ss_total"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="Total"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_itemdisc_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.1"
                    android:gravity="right" >

                    <TextView
                        android:id="@+id/ss_item_disc"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="Item_Disc"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_subTotal_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right" >

                    <TextView
                        android:id="@+id/ss_subTotal"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="Sub Total"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_tax_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right" >

                    <TextView
                        android:id="@+id/ss_tax"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="Tax"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_netTotal_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right" >

                    <TextView
                        android:id="@+id/ss_net_total"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="Net Total"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_exchQty_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/ss_exch_qty"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text=""
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_minsellingprice_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/ss_minselling_price"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text=""
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/itemremarks_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/ss_item_remarks"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text=""
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_currentQty_layout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/ss_current_qty"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ss_arrow_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dip"
                    android:gravity="center" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="35dp"
                        android:contentDescription="@string/title_bar_right_button"
                        android:src="@drawable/ic_arrow_right"
                        android:visibility="invisible" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/delete"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="top"
            android:src="@mipmap/close"
            android:paddingLeft="3dp"
            android:visibility="gone"/>

        <ImageView
            android:id="@+id/edit"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="bottom"
            android:src="@mipmap/ic_edit_icon"
             android:paddingLeft="3dp"
            android:visibility="gone"/>
    </FrameLayout>

</LinearLayout>